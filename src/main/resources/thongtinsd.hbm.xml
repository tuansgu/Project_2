<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="DAL.thongtinsd" table="thongtinsd">      
        <id name="maTT" type="int" column="MaTT">
            <generator class="increment"/>
        </id>
        <property name="maTV" column="MaTV" type="int"/>
        
        <property name="maTB" column="MaTB" type="int"/>

        <property name="tgVao" column="TGVao" type="timestamp"/>
        
        <property name="tgMuon" column="TGMuon" type="timestamp"/>
        
        <property name="tgTra" column="TGTra" type="timestamp"/>
        
        <property name="tgDatCho" column="tgDatCho" type="timestamp"/>

    </class>
</hibernate-mapping>
